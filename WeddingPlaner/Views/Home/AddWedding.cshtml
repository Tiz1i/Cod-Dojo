@model Wedding
<div class="row">
    <div class="col-sm-6">
        <h1>Plan your wedding</h1>
    </div>
     <div class=" offset-sm-3 col-sm-3">
        <a asp-action="Dashboard" asp-controller="Home">Dashboard</a>
        <a asp-action="LogOut" asp-controller="Home">Log out</a>
    </div>
</div>
<div class="row">
    <div class="col-sm-6">
        
<form  asp-controller="Home" asp-action="CreateWedding"  method="post">

 <div class="form-group">
    <span asp-validation-for="WedderOne"></span>
    <label asp-for="WedderOne"></label>
    <input asp-for="WedderOne" class="form-control" >
   
  </div>
  <div class="form-group">
    <span asp-validation-for="WedderTwo"></span>
    <label asp-for="WedderTwo"></label>
    <input asp-for="WedderTwo" class="form-control" >
   
  </div>

 <div class="form-group">
    <span asp-validation-for="Date"></span>
    <label asp-for="Date"></label>
    <input asp-for="Date" class="form-control" >
   
  </div>
   <div class="form-group">
    <span asp-validation-for="Address"></span>
    <label asp-for="Address"></label>
    <input asp-for="Address" class="form-control" >
   
  </div>

  
    <button type="submit" class="btn btn-primary mt-3">Create</button>
</form>


</div>

    